<div class="center jumbotron">
  <h1>Welcome to Bookbook</h1>
  <% if !user_signed_in? %>
  <h2>
    Sign up and start getting your reading lists now!
  </h2>
  <%= link_to "Sign up now!", new_user_registration_path, class: "btn btn-lg btn-primary" %>
  <% end %>
</div>

<% if user_signed_in? %>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Age</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>Jill</td>
      <td>Smith</td>
      <td>50</td>
    </tr>
    <tr>
      <td>Eve</td>
      <td>Jackson</td>
      <td>94</td>
    </tr>
  </tbody>
</table>
<% end %>


<div><%#= image_tag("kitten.jpg", alt: "Kitten", width: "200")%></div>
<%= link_to image_tag("rails.png", alt: "Rails logo", width: "200"), "https://rubyonrails.org/" %>